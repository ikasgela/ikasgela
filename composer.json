{
    "name": "ikasgela/ikasgela",
    "description": "ikasgela.com",
    "keywords": [
        "ikasgela",
        "laravel"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ion Jaureguialzo Sarasola",
            "email": "ion@jaureguialzo.com",
            "homepage": "https://www.jaureguialzo.com",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-redis": "*",
        "ext-zip": "*",
        "akaunting/laravel-setting": "^1.0",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "bkwld/cloner": "^3.4",
        "browner12/helpers": "^3.0",
        "cmgmyr/messenger": "^2.17",
        "cohensive/embed": "5.5.*",
        "consoletvs/charts": "6.*",
        "doctrine/dbal": "^3.0",
        "eduardokum/laravel-mail-auto-embed": "^2.0",
        "graham-campbell/markdown": "^14.0",
        "hedii/laravel-gelf-logger": "^7.0",
        "ikasgela/laravel-gitea": "^1.0",
        "intervention/image": "^2.5",
        "josiasmontag/laravel-recaptchav3": "^1.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.2",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.1",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "^6.0",
        "laraveles/spanish": "^1.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "lukeraymonddowning/honey": "^0.4",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.7",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "spatie/laravel-activitylog": "^4.0",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "stechstudio/laravel-zipstream": "^4.4",
        "symfony/http-client": "^6.2",
        "symfony/mailgun-mailer": "^6.2",
        "titasgailius/terminal": "^1.0",
        "watson/rememberable": "^6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "brianium/paratest": "^6.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7.0",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar",
                "beyondcode/laravel-dump-server"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
