{
    "name": "ikasgela/ikasgela",
    "description": "ikasgela.com",
    "keywords": [
        "ikasgela",
        "laravel"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ion Jaureguialzo Sarasola",
            "email": "ion@jaureguialzo.com",
            "homepage": "https://www.jaureguialzo.com",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-redis": "*",
        "akaunting/laravel-setting": "^1.0",
        "bkwld/cloner": "^3.4",
        "browner12/helpers": "^3.0",
        "cmgmyr/messenger": "^2.17",
        "cohensive/embed": "5.5.*",
        "consoletvs/charts": "6.*",
        "doctrine/dbal": "^3.0",
        "eduardokum/laravel-mail-auto-embed": "^1.0",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "^13.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hedii/laravel-gelf-logger": "^6.0",
        "intervention/image": "^2.5",
        "jenssegers/mongodb": "^3.8",
        "josiasmontag/laravel-recaptchav3": "^0.6.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.2",
        "lab404/laravel-impersonate": "^1.6",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "laravel/legacy-factories": "^1.1",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "laraveles/spanish": "^1.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "lukeraymonddowning/honey": "^0.3.1",
        "maatwebsite/excel": "^3.1",
        "niklasravnsborg/laravel-pdf": "^4.0",
        "php-http/guzzle7-adapter": "^0.1",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "spatie/laravel-activitylog": "^3.3",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "watson/rememberable": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "brianium/paratest": "^6.0",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.0",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "wulfheart/pretty_routes": "^0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
